{
  "name": "bit-heroes",
  "version": "1.0.0",
  "description": "Bit Heroes data",
  "main": "src/index.js",
  "scripts": {
    "scrapping": "node -r babel-register scrapping/index.js",
    "build_js": "babel src/javascripts --out-file public/script.js --source-maps",
    "build_css": "cleancss -o public/style.css src/css/* --source-map",
    "build": "rm -rf public && mkdir public && npm run build_js && npm run build_css",
    "watch_js": "npm run build_js -- -w",
    "watch_css": "watch -n 0.5 -x npm run build_css",
    "dev": "npm run build && DEBUG=bit-heroes nodemon -- src/index.js",
    "start": "npm run build && node -r babel-register src/index.js"
  },
  "keywords": [],
  "author": "NerOcrO",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=3.8.6"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "clean-css-cli": "^4.2.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.9.0",
    "debug": "^3.2.6",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "helmet": "^3.15.1",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NerOcrO/bit-heroes"
  }
}
